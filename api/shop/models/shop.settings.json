{
  "connection": "default",
  "collectionName": "shops",
  "info": {
    "name": "Shop"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 25
    },
    "logo": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true,
      "minLength": 50
    },
    "email": {
      "type": "email",
      "required": true
    },
    "contact": {
      "type": "biginteger",
      "required": true
    },
    "website": {
      "type": "string"
    },
    "facebook": {
      "type": "string"
    },
    "instagram": {
      "type": "string"
    },
    "linkedin": {
      "type": "string"
    },
    "youtube": {
      "type": "string"
    },
    "media": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": true
    },
    "published": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "times": {
      "type": "json",
      "required": true
    },
    "additional": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    }
  }
}
